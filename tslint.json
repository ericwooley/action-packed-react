{
  "extends": ["tslint-react", "tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "no-empty": false,
    "max-line-length": [true, 120],
    "jsx-wrap-multiline": true
  },
  "linterOptions": {
    "include": ["./**/src/**/*.{ts,tsx}"],
    "exclude": [
        "node_modules/**/*",
        "**/*.d.ts"
    ]
}
}
