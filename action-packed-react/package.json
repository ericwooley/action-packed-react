{
  "name": "action-packed-react",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "./index.ts",
  "module": "./index.ts",
  "author": "Eric Wooley <ericwooley@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "yarn tsc",
    "start": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare"
  },
  "lint-staged": {
    "{src,test}/**/*.tsx?": [
      "tslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "dist",
      "node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/rollup.config.ts",
      "tools",
      "dist"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 70,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "colors": "^1.3.3",
    "commitizen": "^4.0.3",
    "core-js": "^3.1.4",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "dtslint": "^0.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-config": "^24.8.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.136",
    "@types/lodash.curry": "^4.1.6",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "history": "^4.9.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "16",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "typescript": "^3.5.3"
  }
}
