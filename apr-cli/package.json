{
  "name": "apr-cli",
  "version": "1.0.0",
  "description": "Cli for action packed react",
  "main": "dist/index.js",
  "author": "Eric Wooley <ericwooley@gmail.com>",
  "bin": {
    "apr": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc --watch",
    "test": "yarn jest --runInBand --no-cache"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testSequencer": "./testSequencer.js",
    "testRegex": "(.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "dist",
      "node_modules"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.2.1",
    "debug": "^4.1.1",
    "enquirer": "^2.3.1",
    "file-loader": "^4.1.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "hygen": "^4.0.8",
    "supports-color": "^7.0.0",
    "ts-loader": "^6.0.4",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "which": "^1.3.1",
    "yargs-parser": "^13.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/debug": "^4.1.4",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.7.2",
    "@types/which": "^1.3.1",
    "@types/yargs-parser": "^13.0.0",
    "axios": "^0.19.0",
    "jest": "^24.9.0",
    "mock-stdin": "^0.3.1",
    "tree-kill": "^1.2.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
