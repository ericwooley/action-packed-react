{
  "name": "apr-cli",
  "version": "0.0.0",
  "description": "Cli for action packed react",
  "main": "dist/index.js",
  "author": "Eric Wooley <ericwooley@gmail.com>",
  "bin": {
    "apr": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc --watch",
    "test": "yarn jest --clearCache && yarn jest --runInBand --no-cache",
    "lint": "tslint  --project tsconfig.json"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testSequencer": "./testSequencer.js",
    "testRegex": "(.*(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "dist",
      "node_modules"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "ast-types": "^0.13.2",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "cosmiconfig": "^6.0.0",
    "debug": "^4.1.1",
    "enquirer": "^2.3.2",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "hygen": "^5.0.3",
    "jscodeshift": "^0.6.4",
    "react-docgen-typescript-loader": "^3.6.0",
    "remeda": "^0.0.14",
    "source-map-loader": "^0.2.4",
    "supports-color": "^7.1.0",
    "tslint": "^5.20.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "which": "^2.0.2",
    "yargs-parser": "^16.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.4",
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.23",
    "@types/jscodeshift": "^0.6.3",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.12.14",
    "@types/supports-color": "^5.3.0",
    "@types/typescript": "^2.0.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/which": "^1.3.2",
    "@types/yargs-parser": "^13.1.0",
    "axios": "^0.19.0",
    "chokidar": "^3.3.0",
    "jest": "^24.9.0",
    "jscodeshift-helper": "^1.1.0",
    "md5": "^2.2.1",
    "mock-stdin": "^1.0.0",
    "tree-kill": "^1.2.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
}
